{
  "expo": {
    "name": "wallpaper-app",
    "slug": "wallpaper-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F0F0F"
    },
    "androidStatusBar": {
      "backgroundColor": "#0F0F0F",
      "barStyle": "light-content"
    },
    "iosStatusBar": {
      "backgroundColor": "#0F0F0F",
      "barStyle": "light-content"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0F0F0F"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
